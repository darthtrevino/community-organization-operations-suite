{
  "name": "@cbosuite/api",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "scripts": {
    "clean": "essex clean .version dist/ deploy.zip",
    "start:db": "docker-compose up -d ",
    "start_api": "DEBUG='cbosuite:*' nodemon src/index.ts",
    "start:api": "yarn start_api",
    "start:api:static": "DEBUG='cbosuite:*' yarn node dist/src/index.js",
    "start": "yarn start:api",
    "build": "run-s compile write_deploy_package",
    "build:api": "yarn build",
    "archive": "./scripts/archive.sh",
    "archive:api": "yarn archive",
    "compile": "ttsc -b tsconfig.json",
    "test": "jest",
    "copy_mock_data": "shx mkdir -p dist/mock_data/ && shx cp -r mock_data/* dist/mock_data/",
    "assets": "run-s create_mock_data copy_mock_data write_version",
    "tsn": "ts-node -r tsconfig-paths/register",
    "create_mock_data:": "yarn tsn scripts/createMockData",
    "create_seed_data:": "yarn tsn createSeedData.ts",
    "write_version": "node scripts/writeVersion",
    "deploy:integ": "az webapp deployment source config-zip --resource-group greenlight --name greenlight-api-integ --src deploy.zip",
    "deploy:prod": "az webapp deployment source config-zip --resource-group greenlight --name greenlight-api --src deploy.zip",
    "write_deploy_package": "yarn tsn writeDeployPackage.ts",
    "migrate": "yarn tsn ./migrateMongo.ts",
    "migrate:db:up": "yarn migrate up"
  },
  "dependencies": {
    "@cbosuite/schema": "workspace:packages/schema",
    "@graphql-tools/schema": "^8.2.0",
    "@graphql-tools/utils": "^8.2.3",
    "apollo-server": "^3.3.0",
    "apollo-server-core": "^3.3.0",
    "apollo-server-errors": "^3.2.0",
    "apollo-server-fastify": "^3.3.0",
    "applicationinsights": "^2.1.7",
    "bcrypt": "^5.0.1",
    "config": "^3.3.6",
    "dataloader": "^2.0.0",
    "debug": "^4.3.2",
    "detect-libc": "^1.0.3",
    "dotenv": "^10.0.0",
    "fastify": "^3.21.6",
    "fastify-cors": "^6.0.2",
    "firebase-admin": "^9.11.1",
    "graphql": "^15.6.0",
    "graphql-subscriptions": "^1.2.1",
    "i18n": "^0.13.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "make-dir": "^3.1.0",
    "migrate-mongo": "^8.2.3",
    "mongodb": "3.6.12",
    "node-fetch": "^2.6.5",
    "nodemailer": "^6.6.5",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "nopt": "^5.0.0",
    "npmlog": "^5.0.1",
    "pino": "^6.13.3",
    "pino-pretty": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "semver": "^7.3.5",
    "subscriptions-transport-ws": "^0.9.19",
    "tar": "^6.1.11",
    "tsyringe": "^4.6.0",
    "uuid": "^8.3.2",
    "ws": "^8.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.16.0",
    "@babel/plugin-proposal-optional-chaining": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@essex/jest-config": "^18.0.1",
    "@essex/scripts": "^18.2.0",
    "@tsconfig/node14": "^1.0.1",
    "@types/babel__core": "^7",
    "@types/babel__preset-env": "^7",
    "@types/bcrypt": "^5.0.0",
    "@types/config": "^0.0.39",
    "@types/copy": "^0.3.2",
    "@types/debug": "^4",
    "@types/faker": "^5.5.8",
    "@types/i18n": "^0.13.1",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/lodash": "^4.14.173",
    "@types/migrate-mongo": "^8.1.2",
    "@types/mongodb": "^3.6.20",
    "@types/node": "^14.17.18",
    "@types/nodemailer": "^6.4.4",
    "@types/pino": "^6.3.11",
    "@types/pino-pretty": "^4.7.1",
    "@types/uuid": "^8.3.1",
    "@types/ws": "^7.4.7",
    "copy": "^0.3.2",
    "core-js": "^3.19.0",
    "faker": "^5.5.3",
    "jest": "^27.2.1",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5",
    "shx": "^0.3.3",
    "supports-color": "^9.0.2",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "tsconfig-paths-jest": "^0.0.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1"
  }
}
